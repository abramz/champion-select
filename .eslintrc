{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-console": 0,
    "max-len": 0,
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "react/require-extension": 0,
    "react/jsx-filename-extension": 0,
    "import/extensions": 2,
    "import/no-extraneous-dependencies": 0,
    "import/imports-first": 0,
    "generator-star-spacing": 0
  }
}
