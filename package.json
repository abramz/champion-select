{
  "name": "champion-select",
  "description": "Simulate build breakpoints for champions in League of Legends",
  "version": "0.0.1",
  "directories": {
    "doc": "docs",
    "lib": "src",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abramz/champion-select.git"
  },
  "author": "Andrew Shapro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abramz/champion-select/issues"
  },
  "homepage": "https://github.com/abramz/champion-select#readme",
  "engines": {
    "node": ">=5.0 <7",
    "npm": ">=3.3 <4"
  },
  "scripts": {
    "eslint": "eslint src tools test",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "cross-env NODE_ENV=test mocha test/spec --require test/setup.js --compilers js:babel-register --recursive --reporter spec",
    "test:watch": "npm run test -- --reporter min --watch",
    "test:cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm test",
    "test:ci": "npm run snyk:test && npm run test:cover",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "deploy": "babel-node tools/run deploy",
    "render": "babel-node tools/run render",
    "start": "babel-node tools/run start",
    "snyk:wizard": "snyk wizard",
    "snyk:protect": "snyk protect",
    "snyk:test": "snyk test"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/server/data/types"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
